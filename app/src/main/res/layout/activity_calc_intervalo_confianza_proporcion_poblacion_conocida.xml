<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".CalcIntervaloConfianzaProporcionPoblacionDesconocida">

    <TextView
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:text="@string/p_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_z"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_p"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_sample"
        tools:text="@string/p_equals" />

    <TextView
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_population"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:text="@string/N_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_sample"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_population"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/N_equals" />

    <TextView
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:text="@string/n_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_p"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_sample"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_population"
        tools:text="@string/n_equals" />

    <TextView
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_z"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:text="@string/Z_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_ic"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_z"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_p"
        tools:text="@string/Z_equals" />

    <TextView
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_ic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="84dp"
        android:text="@string/IC_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_ic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_z"
        tools:text="@string/IC_equals" />

    <EditText
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_p"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_edit_text"
        android:ems="10"
        android:hint="@string/p"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_z"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_p"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_sample" />

    <EditText
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_population"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_edit_text"
        android:ems="10"
        android:hint="@string/N"
        android:importantForAutofill="no"
        android:inputType="number"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_sample"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_population"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_sample"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_edit_text"
        android:ems="10"
        android:hint="@string/n"
        android:importantForAutofill="no"
        android:inputType="number"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_p"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_n_sample"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_n_population" />

    <EditText
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_z"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_edit_text"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="10"
        android:enabled="false"
        android:focusableInTouchMode="false"
        android:hint="@string/Z"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:text="@string/Z_val"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_ic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_z"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_p" />

    <EditText
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_ic"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="96dp"
        android:background="@drawable/round_edit_text"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="10"
        android:enabled="false"
        android:focusableInTouchMode="false"
        android:hint="@string/IC"
        android:importantForAutofill="no"
        android:inputType="text"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_tv_ic"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_et_z" />

    <Button
        android:id="@+id/activity_calc_intervalo_confianza_proporcion_poblacion_conocida_btn_clear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="@string/clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="@string/clear" />
</androidx.constraintlayout.widget.ConstraintLayout>