<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".CalcDistBinomial">

    <TextView
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_n"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="@string/n_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_p"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_n"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/n_equals" />

    <TextView
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="@string/p_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_pi"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_p"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_n"
        tools:text="@string/p_equals" />

    <TextView
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_pi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:text="@string/pi_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_z"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_pi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_p"
        tools:text="@string/pi_equals" />

    <TextView
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_z"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="24dp"
        android:text="@string/Z_equals"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_spn_prob"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_z"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_pi"
        tools:text="@string/Z_equals" />

    <EditText
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_n"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/round_edit_text"
        android:ems="10"
        android:hint="@string/n"
        android:importantForAutofill="no"
        android:inputType="number"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_p"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_n"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_p"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/round_edit_text"
        android:ems="10"
        android:hint="@string/p"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_pi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_p"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_n" />

    <EditText
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_pi"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/round_edit_text"
        android:ems="10"
        android:hint="@string/pi"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_z"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_pi"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_p" />

    <EditText
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_z"
        android:layout_width="180dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/round_edit_text"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="10"
        android:focusableInTouchMode="false"
        android:hint="@string/Z"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_prob"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_z"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_pi" />

    <Spinner
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_spn_prob"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="64dp"
        android:backgroundTint="#000000"
        android:entries="@array/x_probs_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_prob"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_tv_z" />

    <EditText
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_prob"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="88dp"
        android:background="@drawable/round_edit_text"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="10"
        android:focusableInTouchMode="false"
        android:hint="@string/P"
        android:importantForAutofill="no"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_spn_prob"
        app:layout_constraintTop_toBottomOf="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_et_z" />

    <Button
        android:id="@+id/activity_calc_prueba_hipotesis_proporcion_poblacional_btn_clear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="@string/clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="@string/clear" />
</androidx.constraintlayout.widget.ConstraintLayout>